<script>
    var reqDoc = document.querySelector('.product-image');
    var smallDoc = document.querySelectorAll('.product-image-thumb');
    var small = document.querySelectorAll('.small-product-thumb');
    smallDoc[0].classList.add('active');  
    // next phase
    smallDoc.forEach(element => {
        element.addEventListener('click', ()=>{
            if(!element.classList.contains('active')){
                element.classList.add('active');
                var reqParent = element.parentElement.children;
                for (let index = 0; index < reqParent.length; index++) {
                    const need = reqParent[index];
                    need.classList.remove('active');
                };
                element.classList.add('active');
                reqDoc.src = element.firstChild.src;
            }
        });
    });
    //control radion buttons
    var colourButton = document.getElementById('colourLabel');
    var needed = colourButton.children;
    for (let index = 0; index < needed.length; index++) {
    const col = needed[index];
    console.log(col);
    col.addEventListener('click',()=>{
        if(col.classList.contains('focus')){
            col.style.backgroundColor = "lightgray";
        }
    });
    }
</script>

